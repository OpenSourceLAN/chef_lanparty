server {
   listen 80;
   listen [::]:80;

   server_name <%= @servername %>;

   root <%= @docroot %>;
   index index.php index.html;

   location / {
       try_files $uri $uri/ =404;
	}
    location ~ \.php$ {
    	include snippets/fastcgi-php.conf;
        fastcgi_pass unix:/var/run/php/php7.0-fpm.sock;
    }

}
